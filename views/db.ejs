<!DOCTYPE html>
<html>
<head>
  
    <link rel="stylesheet" href="css/style.css">
</head>

<body>



<div class="container">
<h2>Search results</h2>
<% include /partials/header %>
<ul>
    <div class="row"></div>
    <% results.forEach(function(r) { %>
        
            <div class="column">
            
        <li><img src = <%= r.imgsrc %> > - <%= r.lastname %> - <%= r.firstname %> - <%= r.companyname %> - <%= r.companydescription %></li>
    <% }); %> </div>
</ul>

</div>

</body>
</html>
